@model List<DefectLoggingApp.Models.DefectRecord>

@foreach (var defect in Model)
{
    <tr>
        <td>@defect.PrLine</td>
        <td>@defect.Surface</td>
        <td>@defect.StartWidth</td>
        <td>@defect.EndWidth</td>
        <td>@defect.StartLength</td>
        <td>@defect.EndLength</td>
        <td>@defect.TotalLength</td>
        <td>@defect.Type</td>
        <td>@defect.DefectName</td>
        <td>@defect.Severity</td>
    </tr>
}
